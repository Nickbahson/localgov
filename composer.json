{
    "name": "localgovdrupal/localgov",
    "description": "LocalGovDrupal distribution",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/localgovdrupal/localgov",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:localgovdrupal/localgov_services.git"
        }
    ],
    "require": {
        "drupal/admin_toolbar": "^2.2",
        "drupal/core": "^8.8",
        "drupal/console": "~1.0",
        "drupal/devel": "^2.1",
        "drush/drush": "~9",
        "localgovdrupal/localgov_core": "dev-master",
        "localgovdrupal/localgov_services": "dev-develop"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
    }
}
